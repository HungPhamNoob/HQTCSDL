
DROP TABLE IF EXISTS staff_salary_history;

CREATE TABLE staff_salary_history (
    staff_id INT NOT NULL,
    salary DECIMAL(10,2) NOT NULL,
    start_date DATE NOT NULL,
    end_date DATE DEFAULT '9999-12-31',  -- Mặc định vô hạn (hiện tại vẫn còn hiệu lực)
    CHECK (start_date < end_date)        -- Ràng buộc logic thời gian
);


-- Xóa cột time_range nếu đã tồn tại
ALTER TABLE staff_salary_history DROP COLUMN IF EXISTS time_range;

-- Xóa Index cũ nếu đã tồn tại
DROP INDEX IF EXISTS idx_staff_salary_btree;
DROP INDEX IF EXISTS idx_staff_salary_gist;

-- Thêm cột mới dạng khoảng thời gian
ALTER TABLE staff_salary_history ADD COLUMN time_range tsrange;

-- Gán giá trị cho cột mới
UPDATE staff_salary_history
SET time_range = tsrange(start_date, end_date, '[]');


CREATE INDEX idx_salary_history_daterange ON staff_salary_history 
USING gist (daterange(start_date, end_date, '[]'));


/*
CREATE INDEX idx_staff_salary_history_staff_id_start_date ON staff_salary_history(staff_id, start_date);
*/

